{
  "name": "mithras-mobile",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "MoproReactNativeBindings"
  ],
  "scripts": {
    "assets": "npx react-native-asset",
    "ubrn:ios": "cd MoproReactNativeBindings && npm run ubrn:ios",
    "ubrn:android": "cd MoproReactNativeBindings && npm run ubrn:android",
    "prebuild": "npm run assets && bundle install && (cd ios && bundle exec pod install)",
    "ios": "npm run prebuild && react-native run-ios",
    "android": "npm run assets && react-native run-android",
    "start": "react-native start",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a\"",
    "build:ios": "react-native build-ios --mode Debug",
    "gen:logo": "npx react-native set-icon --path ./src/assets/logo.png && node ./scripts/sync-android-round-icons.mjs"
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.81.4",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-fs": "^2.20.0",
    "react-native-safe-area-context": "^5.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@bam.tech/react-native-make": "^3.0.3",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.4",
    "@react-native/metro-config": "0.81.4",
    "@react-native/typescript-config": "0.81.4",
    "@types/react": "^19.1.0",
    "react-native-builder-bob": "^0.40.13",
    "react-native-monorepo-config": "^0.1.9"
  },
  "engines": {
    "node": "^20.19.0 || ^22.12.0 || >= 23.4.0",
    "npm": ">=10.8.2"
  }
}